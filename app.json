{
  "expo": {
    "name": "Cr4ck",
    "slug": "cr4ck",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "splash": {
        "mdpi": "./assets/img/splash/splash-mdpi.png",
        "hdpi": "./assets/img/splash/splash-hdpi.png",
        "xhdpi": "./assets/img/splash/splash-xhdpi.png",
        "xxhdpi": "./assets/img/splash/splash-xxhdpi.png",
        "xxxhdpi": "./assets/img/splash/splash-xxxhdpi.png",
        "resizeMode": "contain"
      }
    }
  }
}
